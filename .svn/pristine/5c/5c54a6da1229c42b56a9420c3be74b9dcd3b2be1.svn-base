      color:black;
      font-size: 1.1rem;
      margin-bottom: 10px;
    }

    .description {
      font-size: 0.9rem;
      color: #333;
      margin-bottom: 10px;
    }

    .stars {
      color: #ffd700;
      font-size: 2.2rem;

      span {
        margin: 0 1px;
        color: #ccc;

        &.filled {
          color: #7020D2;
                  }
      }
    }
  }

  .carousel-buttons {
    display: flex;
    justify-content: left;
    margin-top: 5px;
    padding-left: 130px;
    gap: 2px;

    button {
      width: 60px;
      height: 60px;
      border: 1px solid #ccc;
      background: white;
      border-radius: 3px;
      font-size: 1.2rem;
      cursor: pointer;

      &.active {
        border-color: #ee9229;
        color: #f88c27;
      }

      span {
        display: inline-block;
        transform: translateY(-2px);
      }
    }
  }
}
