.vision-cards-wrapper {
  padding: 60px 20px;
  display: flex;
  justify-content: center;
}

.vision-cards {
  display: flex;
  gap: 20px;
  flex-wrap: wrap;
  justify-content: center;
}

.flip-card {
  background-color: transparent;
  width: 280px;
  height: 200px;
  perspective: 1000px;

  .flip-card-inner {
    position: relative;
    width: 100%;
    height: 100%;
    text-align: center;
    transition: transform 0.6s;
    transform-style: preserve-3d;
  }

  &:hover .flip-card-inner {
    transform: rotateY(180deg);
  }

  .flip-card-front,
  .flip-card-back {
    position: absolute;
    width: 100%;
    height: 100%;
    padding: 30px;
    backface-visibility: hidden;
    border-radius: 10px;
    box-shadow: 0 6px 12px rgba(0, 0, 0, 0.1);
    color: #fff;
    display: flex;
    flex-direction: column;
    justify-content: center;
  }

  .flip-card-front h3,
  .flip-card-back h3 {
    margin-bottom: 10px;
  }

  .flip-card-front {
    background-color: #007bff;
  }

  .flip-card-back {
    background-color: #333;
    transform: rotateY(180deg);
  }

  // Theme colors
  .blue {
    background-color: #007bff;
  }

  .green {
    background-color: #28a745;
  }

  .orange {
    background-color: #fd7e14;
  }
}
.section-subtitle {
  font-size: 2.3rem;
  font-weight: 200; /* light */
  color: #000;
  margin-bottom: 20px;

  strong {
    font-weight: 700; /* bold */
  }
}


