  }
}


// Story section

.story-section {
  background-image: url('/assets/images/about-page/We-Started.png');
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
  // background-size: 100% 600px;
  // max-width: 3000px;
  max-width: 100%;
  padding-top: 100px;
  padding-bottom: 100px;
  padding-left: 82px;
  position: relative;
  color: white;
  border-top-left-radius: 3%;
  border-top-right-radius: 3%;
  // justify-content: center;
  display: flex;
}

.overlay {
  display: flex;
  padding-top: 20px;
  border-radius: 8px;
  // margin: auto;
  // padding-left: 50px;
   font-family: 'Geist',sans-serif !important;
}

.story-content h2 {
  font-size: 50px;
  margin-bottom: 20px;
}

.story-content p {
  font-size: 18px;
  line-height: 1.6;
  margin-bottom: 20px;
  color: #e0e0e0;
}

.watch-btn {
  // padding: 20px 35px;
  padding-left: 40px;
  background: linear-gradient(#0A47B9 0%, #763289 100%);
  border: none;
  color: white;
  font-size: 20px;
  border-radius: 40px;
  cursor: pointer;
  transition: 0.3s ease;
}

/* ✅ Responsive: Tablet */
@media (max-width: 1024px) {
  .story-section {
    padding-left: 40px;
    padding-right: 40px;
    padding-top: 80px;
    padding-bottom: 80px;
  }

  .overlay {
    padding-left: 30px;
    padding-right: 30px;
  }

  .story-content h2 {
    font-size: 36px;
    text-align: center;
  }

  .story-content p {
    font-size: 16px;
    text-align: center;
  }

  .watch-btn {
    display: block;
    margin: 0 auto;

  }
}

/* ✅ Responsive: Mobile */
@media (max-width: 600px) {
  .story-section {
    padding: 60px 20px;
  }

  .overlay {
    padding: 0;
  }

  .story-content h2 {
    font-size: 28px;
  }

  .story-content p {
    font-size: 14px;
    line-height: 1.5;
  }

  .watch-btn {
    font-size: 16px;
    padding-left: 15px;
    

  }
}
