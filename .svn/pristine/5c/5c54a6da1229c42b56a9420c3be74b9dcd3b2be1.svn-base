// vision section

.vision-section {
  justify-content: center;
    background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
  max-width: 100%;
  // padding: 60px 20px;
  padding-left: 120px;
  padding-top: 60px;
  background-color: #fafeffe5;
}

.vision-container {
   font-family: 'Geist',sans-serif !important;
  padding-top: 50px;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  gap: 40px;
}

.vision-text {
  flex: 1;
  max-width: 450px;
}

.vision-text h2 {
   font-family: 'Geist',sans-serif !important;
  font-size: 2.8rem;
  font-weight: 100;
}

.vision-text h2 span {
   font-family: 'Geist',sans-serif !important;
  font-weight: 600;
  color: #000;
}

.vision-subheading {
   font-family: 'Geist',sans-serif !important;
  font-weight: 500;
  margin-top: 20px;
  font-size: 1.3rem;
}

.vision-description {
   font-family: 'Geist',sans-serif !important;
  margin-top: 10px;
  font-size: 1rem;
  color: #444;
  font-weight: 400;
}

.arrow-button {
  margin-top: 60px;
  display: flex;
  // padding: 10px 10px;
  // gap: 0px;
  padding-left: 10px;

 gap: 2px;
}

.arrow-btn {
   width: 60px;
   height: 60px;
  font-size: 1.2rem;
  border: 1px solid #F24E1E;
  background-color: transparent;
  color:#F24E1E;
  border-radius: 1px;
  cursor: pointer;
  transition: all 0.3s;
}

.arrow-btn:hover {
  background-color: #ff4d30;
  color: #fff;
}

.vision-cards {
  flex: 2;
  display: flex;
  gap: 20px;
  flex-wrap: wrap;
  justify-content: center;
}

.vision-card {
  flex: 1;
  min-width: 220px;
  max-width: 280px;
  height: 300px;
  padding: 60px;
  border-radius: 16px;
  box-shadow: 0 8px 20px rgba(0, 0, 0, 0.08);
  transition: transform 0.3s;
}

.vision-card h3 {
  font-size: 1.8rem;
  font-weight: bold;
  margin-bottom: 10px;
}
.vision-card p {
  font-size: 1rem;
  color: #444;
}
.vision-card:hover {
  transform: translateY(-5px);
}

.vision-card.blue {

  background-image: url("/assets/images/about-page/Rectangle 1061.png");
  // background: linear-gradient(#B2EEFC 100%,#C2E1FD 0%);
}

.vision-card.green {
  background-image: url("/assets/images/about-page/Rectangle 1062.png");
}

.vision-card.orange {
  background-image: url("/assets/images/about-page/Rectangle 1064.png");
}

@media (max-width: 768px) {
  .vision-container {
    flex-direction: column;
    padding-left: 10px;
  }

  .vision-text {
    max-width: 100%;
    padding: 0 10px;
  }

  .vision-cards {
    justify-content: center;
  }
}

// belief section

.belief-section {
   background-size: cover;
  justify-content: center;
  padding: 60px;
  // padding-left: -10px;
  gap: 30px;
  max-width: 100%;


  background-image: url("/assets/images/about-page/Rectangle 1060.png");

  .section-header {
     font-family: 'Geist',sans-serif !important;
    text-align: center;
    width: auto;
    margin: 0 auto 40px;

    h2 {
      font-size: 2.7rem;

      .light {
        font-weight: 140;
      }

      strong {
        font-weight: 550;
      }
    }
  }
 
  .description-belief{
     font-family: 'Geist',sans-serif !important;
    text-align: center;
    p{
      margin: 0;
    }
    .description1,
    .description2{
      padding: 0;
      
    }}

  .belief-cards {
     font-family: 'Geist',sans-serif !important;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    gap: 30px;

    .card {
      display: flex;
      background: #fff;
      border-radius: 6px;
      padding: 40px 30px;
      width: 400px; 
      height: 400px;
      text-align: left;
      box-shadow: 0 4px 10px rgba(0, 0, 0, 0.05);
      transition: transform 0.3s;
      margin-bottom: 0px;

      &:hover {
        transform: translateY(-5px);
      }
      img {
        text-align: left;
        
        width: 160px;
        // padding-left: -30PX;
        height: 150px;
        // object-fit: contain;
        margin-bottom: 20px;
      }

      h4 {
        font-family: 'Geist',sans-serif !important;
        font-weight: 700;
        font-size: 1.4rem;
        margin-bottom: 12px;
        text-transform: uppercase;
        position: relative;

        &::after {
          content: '';
          font-family: 'Geist',sans-serif !important;
          display: block;
          width: 40px;
          height: 3px;
          background-color: #7020D2;
          margin-top: 6px;
        }
      }

      p {
        font-size: 1.0rem;
        // margin-bottom: 10px;
        color: #100b0b;
      }

      ul {
        font-size: 1.0rem;
        padding-left: 20px;
        margin-bottom: 10px;

        li {
          font-size: 1.0rem;
          color:  #100b0b;
          list-style-type: disc;
        }
      }

      .read-more {
        color: #020405;
        font-weight: bold;
        font-size: 0.9rem;
        cursor: pointer;
        text-decoration: none;
      }
    }
  }}
  @media (max-width: 1024px) {
  .belief-section {
    padding: 40px 20px;
    
    .section-header h2 {
      font-size: 2rem;
    }

    .belief-cards {
      gap: 20px;

      .card {
        width: 90%;
        height: auto;
        padding: 20px;
      }

      .card img {
        width: 80px;
        height: 80px;
      }

      .card h4 {
        font-size: 1rem;
      }

      .card p, .card ul li {
        font-size: 0.9rem;
      }
    }
  }
}

@media (max-width: 768px) {
  .belief-section {
    padding: 30px 15px;

    .section-header h2 {
      font-size: 1.8rem;
    }

    .belief-cards {
      flex-direction: column;
      align-items: center;

      .card {
        width: 100%;
        padding: 20px;
      }

      .card img {
        width: 70px;
        height: 70px;
      }
    }
  }
}

@media (max-width: 480px) {
  .belief-section {
    .section-header h2 {
      font-size: 1.5rem;
    }

    .belief-cards .card {
      padding: 15px;
    }
  }
}


.testimonials-section {
   background-size: cover;
  justify-content: center;
  background-image: url('/assets/images/about-page/testimonial-bg.png');
padding-top: 60px;
padding-left: 10px;
padding-bottom: 60px;
max-width: 100%;
 font-family: 'Geist',sans-serif !important;

