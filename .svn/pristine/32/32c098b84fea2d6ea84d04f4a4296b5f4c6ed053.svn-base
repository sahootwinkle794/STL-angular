.governance-section {
  // background: linear-gradient(90deg, #f5fcff, #e8f8ff);
  background-image: url('../../../../../public/assets/images/sakhyam/bg.png');
  // padding: 6rem 2rem;
  padding-top: 5rem;
  // padding-left: 0px;
  padding-bottom: 6rem;
  margin: 0;
  //  text-align: center;
   background-size: cover; /* width height */
  background-repeat: no-repeat;
  background-position: center;


  .governance-content {
    
    display: flex;
    align-items: center;
    justify-content: space-between;
    max-width: 1200px;
    margin: auto;
    // gap: 2rem;
    flex-wrap: wrap;

    .governance-text {
      padding-top: 60px;
      flex: 1;
      min-width: 300px;

      h2 {
        font-family: 'Geist',sans-serif !important;
        font-size: 61px;
        font-weight: 250;
        line-height: 120%;

        span {
          display: block;
          font-weight: 600;
        }
      }

      p {
        font-family: "Outfit", sans-serif !important;
        margin-top: 1rem;
        font-size: 18px;
        line-height: 35px;
        color: #000000;
        font-style: normal;
        font-weight: 400;
      }
    }

    .governance-image {
      padding-top: 40px;
      flex: 1;
      text-align: center;

      img {
        max-width: 120%;
        height: auto;
        // box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);
        border-radius: 8px;
      }
    }
  }
}

/* ✅ Responsive for smaller screens */
@media (max-width: 768px) {
  .governance-section .governance-content {
    flex-direction: column;
    text-align: center;

    .governance-text {
      h2 {
        font-size: 1.8rem;
      }
    }
  }
}





// the sectio for the simplify starts

.society-management {
  padding: 20px;
  // background: linear-gradient(135deg, #f7fcff, #e8faff);
  background-image: url('../../../../../public/assets/images/sakhyam/Secondbg.png');
  // padding: rem 2rem;
  text-align: center;
   background-size: cover; /* width height */
  background-repeat: no-repeat;
  background-position: center;
 

  .cards-wrapper {
    padding-top: 80px;
    display: flex;
    justify-content: center;
    gap: 2rem;
    flex-wrap: wrap;
    max-width: 1200px;
    margin: 0 auto;

    .card {
      padding-top: 100px;
      padding-left: 40px;
      padding-bottom: 40px;
      padding-bottom: 30px;
      background: #fff;
      // padding: 2rem;
      border-radius: 8px;
      box-shadow: 0 5px 15px rgba(0, 0, 0, 0.08);
      width: 48%;
      min-width: 300px;
      text-align: left;

      h3 {
        font-family: 'Geist',sans-serif !important;
        font-size: 22px;
        font-weight: bold;
        margin-bottom: 0.5rem;
        position: relative;
        font-weight: 600;
        line-height: 120%;

        &::after {
          content: '';
          display: block;
          width: 90px;
          height: 3px;
          background: #0064D6;

          margin-top: 8px;
        }
      }

      p {
        font-family: 'Outfit', sans-serif !important;
        font-weight: 400;
        font-size: 15px;
        color: #000000;
        margin: 1rem 0;
      }

      ul {
        padding-left: 1.2rem;
        li {
          margin: 0.5rem 0;
          font-size: 0.95rem;
          line-height: 1.4;
        }
      }
    }
  }

  .nav-arrows {
    margin-top: 2rem;
    display: flex;
    justify-content: left;
  gap: 3px;
    padding-left: 150px;

    .arrow-btn {
      // background: transparent;
      // border: 2px solid #ff4b2b;
      // color: #e6715f;
      // padding: 0.5rem 1rem;
      // border-radius: 6px;
      // font-size: 1.2rem;
      // cursor: pointer;
      // transition: all 0.3s ease;
      border-radius: 3px;
  width: 60px;
  height: 60px;
  font-size: 1.4rem;
  border: 1px solid #F24E1E;
  background-color: transparent;
  color: #F24E1E;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: all 0.3s;

      &:hover,
      &.active {
        // background: #ff4b2b;
        // color: #fff;
         background: transparent;
        // background: #ff4b2b;
        color: #fd5d13;
      }
    }
  }
}

/* ✅ Responsive */
@media (max-width: 768px) {
  .society-management .cards-wrapper {
    flex-direction: column;
    align-items: center;

    .card {
      width: 90%;
    }
  }
}


