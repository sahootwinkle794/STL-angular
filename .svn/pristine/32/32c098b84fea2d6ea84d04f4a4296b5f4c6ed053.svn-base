      margin-top: 80px;
      font-family: 'Geist',sans-serif !important;
      padding-left: 120px;
       text-align: left;
      font-size: 40px;
      font-weight: 250;
      margin-bottom: 2rem;

      span {
        font-weight: 600;
      }
    }
  }

  .features-wrapper {
    padding-left: 50px;
    display: flex;
    text-align: left;
    // justify-content: space-between;
