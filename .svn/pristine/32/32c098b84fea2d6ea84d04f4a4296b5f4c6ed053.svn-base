    gap: 3rem;
    max-width: 1200px;
    margin: 70px;

    .feature {
      flex: 1;
      min-width: 180px;
      max-width: 200px;
      text-align: left;

      img {
        width: 50px;
        margin-bottom: 1rem;
      }

      h3 {
        font-family: 'Geist',sans-serif !important;
        font-size: 1rem;
        font-weight: 600;
        margin-bottom: 0.5rem;

      }

      p {
        font-family: 'Outfit', sans-serif !important;
        font-weight: 400;
        font-size: 0.9rem;
        color: #000000;
      }
    }
  }

  .nav-arrows {
    margin-top: 2rem;
    display: flex;
    justify-content: left;
    // gap: 0.4rem;
    gap: 3px;
    padding-left: 120px;

    .arrow-btn {
    border-radius: 3px;
  width: 60px;
  height: 60px;
  font-size: 1.4rem;
  border: 1px solid #F24E1E;
  background-color: transparent;
  color: #F24E1E;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: all 0.3s;
      &:hover,
      &.active {
        background: transparent;
        // background: #ff4b2b;
        color: #fd5d13;
      }
    }
  }
}

/* ✅ Responsive */
@media (max-width: 900px) {
  .key-features .features-wrapper {
    justify-content: center;
  }
}

@media (max-width: 600px) {
  .key-features .features-wrapper {
    flex-direction: column;
    align-items: center;
  }
}


// the section for the FAQ starts 

.faq-section {
  // background: #f7f7f7;
  // background-image: url('/assets/images/sakhyam/faqbg.pg');
  background: #F5F5F5;

  padding: 8rem 10rem;
  // width: 800px;
  // margin: auto;
  text-align: left;
  // display: flex;
   background-size: cover; /* width height */
  background-repeat: no-repeat;
  background-position: center;

  h2 {
    font-family: 'Geist',sans-serif !important;
    font-size: 2rem;
    font-weight: 600;
    margin-bottom: 2rem;
  }

  .faq {
    .faq-item {
      background: #fff;
      border-radius: 8px;
      margin-bottom: 1rem;
      overflow: hidden;
      box-shadow: 0 3px 8px rgba(0, 0, 0, 0.05);

      &.open .faq-answer {
        max-height: 300px;
        padding: 1rem;
      }

      .faq-question {
        font-family: 'Geist',sans-serif !important;
        width: 100%;
        background: #fff;
        border: none;
        padding: 1.1rem;
        font-size: 20px;
        text-align: left;
        font-weight: 600;
        cursor: pointer;
        display: flex;
        justify-content: space-between;
        align-items: center;

        &:hover {
          background: #f1f1f1;
        }
      }

      .faq-answer {
        font-family: 'Outfit', sans-serif !important;
        max-height: 0;
        // overflow: hidden;
        padding: 0 1rem;
        transition: all 0.3s ease;
        color: #2D2D2D;
        font-size: 0.95rem;
      }
    }
  }
}
