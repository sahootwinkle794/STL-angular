.form-box button {
  height: 60px;
  line-height: 60px;
  border-radius: 40px;
//   background: linear-gradient(180deg, #076689 0%, #032633 100%);
background: linear-gradient(135deg, #0A47B9 0%, #763289 100%);
  padding: 0 30px;
  color: #fff;
  font-size: 20px;
  border: 1px solid #fff;
}

.form-box button:hover {
  color: #fff;
}

