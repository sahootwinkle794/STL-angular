//  the sectio for the Key features starts 


.key-features {
  background: linear-gradient(135deg, #f8fcff, #eefaff);
  padding: 5rem 2rem;
  background-position: center;
  background-repeat: no-repeat;
  background-size:cover ;
 

  .features-header {
