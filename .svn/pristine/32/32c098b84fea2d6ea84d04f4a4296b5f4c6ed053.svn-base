// TRY READY SECTION

.ready-sec{
  // margin-top: 50px;
  background-image: url('../../../../../public/assets/images/sakhyam/dmsbglast.png');
   background-size: 1800px 600px; 
  background-repeat: no-repeat;
  background-position: center;
}


.col-lg-5{
  margin-top: 100px;
}
.col-lg-7{
  margin-top: 100px;
}

a{
  color:#0074BD !important;
}
.img-fluid{
  margin-top: -10px;
  width: 25px;
  height: 15px;

  margin-left: 30px;
}
.ready-sec .main-sec-head {
  color: #000000;
  font-family: 'Geist', sans-serif !important;
  font-weight: 100;
  font-size: 80px;
  width: 670px;
  font-style: thin;

  padding: 0;
  background: transparent;
}

.ready-sec .main-sec-head span {
  font-weight: 700;
  font-size: 80px;
  display: block;
}

.ready-sec p {
font-family: 'Outfit', sans-serif !important;
  font-size: 30px;
  margin-bottom: 0;
  margin-top: 30px;
font-weight: 400;
width: 617px;
}

.form-box {
  margin-left: 150px;
  padding: 20px 40px;
  border-radius: 10px;
  background: #FFFFFF;
  // background: #F5F5F5;
  border: 1px solid #DADADA;
  height: 520px;
  width: 700px;
  border-radius: 20px;
}

.form-box label {
  font-family: 'Exo 2', sans-serif !important;
  font-weight: 500;
  font-style: Medium;
  font-size: 22px;
  text-transform: capitalize;
  margin-bottom: 10px;

}

.form-box .form-control {
  background: #F7F7F7;
  border: 1px solid #D3D6DA;
  width: 600px;
  height: 65px;
  padding: 0 20px;
  line-height: 50px;
  border-radius: 10px;
}
.form-control::placeholder{
    font-family: 'Exo 2', sans-serif !important;
  font-weight: 500;
  font-style: Medium;
  font-size: 22px;
  text-transform: capitalize;

}

.form-box textarea.form-control {
  height: 144px;
  margin-bottom: 20px;
}


